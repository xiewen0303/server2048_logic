package com.game.service.bag.model;

/**
 * 普通道具 因为这个实现简单，字需要用itemTplId做主舰即可
 * Created by xiewen on 2016/11/9.
 */
public class Item  implements  IGoods {
    private long uid ;
    private int tplId;
    private int itemCount;
    private long uuid;

    public int getTplId() {
        return tplId;
    }

    public void setTplId(int tplId) {
        this.tplId = tplId;
    }

    public void setUuid(long uuid) {
        this.uuid = uuid;
    }


    public int getItemCount() {
        return itemCount;
    }

    public void setItemCount(int itemCount) {
        this.itemCount = itemCount;
    }

    public long getUid() {
        return uid;
    }

    public void setUid(long uid) {
        this.uid = uid;
    }

    @Override
    public long getUuid() {
        return this.uuid;
    }

    @Override
    public int getItemTplId() {
        return this.tplId;
    }
}
