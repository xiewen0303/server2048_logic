<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
    "http://www.ibatis.com/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings cacheModelsEnabled="false" enhancementEnabled="false"
		lazyLoadingEnabled="false" errorTracingEnabled="true" maxRequests="32"
		maxSessions="10" maxTransactions="5" useStatementNamespaces="true" />
	<sqlMap resource="db/ibatis/bean/SqlExcutor.xml" />
	
	<sqlMap resource="db/ibatis/bean/HeroBaseInfo.xml" />
	<sqlMap resource="db/ibatis/bean/UuidInfo.xml" />
	<sqlMap resource="db/ibatis/bean/ItemInfo.xml" />
	<sqlMap resource="db/ibatis/bean/HeroOtherData.xml" />
	<sqlMap resource="db/ibatis/bean/CopyInfo.xml" />

</sqlMapConfig>